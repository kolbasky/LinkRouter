{
  "global": {
    "fallbackBrowserPath": "C:\\Program Files\\BraveSoftware\\Brave-Browser\\Application\\brave.exe",
    "fallbackBrowserArgs": "\"{URL}\"",
    "defaultConfigEditor": "C:\\Users\\mtrag\\AppData\\Local\\Programs\\Microsoft VS Code\\code.exe",
    "logPath": "",
    "interactiveMode": false,
    "supportedProtocols": [
      "http",
      "https",
      "ssh",
      "mailto",
      "linkrouter-ext",
      "ftp"
    ]
  },
  "rules": [
    {
      "regex": "https://store.steampowered.com.*",
      "program": "C:\\Program Files (x86)\\Steam\\steam.exe",
      "arguments": "steam://openurl/{URL}"
    },
    {
      "regex": "ssh://(.*@(company1|company2).(com|org))/",
      "program": "wsl.exe",
      "arguments": "-i .ssh/id_rsa1 user1@$1"
    },
    {
      "regex": "ssh://(.*)/",
      "program": "ssh.exe",
      "arguments": "ssh -i .ssh/id_rsa_personal biba@$1"
    },
    {
      "regex": "mailto:.*@(company1|company2).(com|ru)",
      "program": "C:\\Program Files\\Microsoft Office\\root\\Office16\\OUTLOOK.EXE",
      "arguments": "https://mail.google.com/mail/?view=cm\u0026fs=1\u0026to=$1"
    },
    {
      "regex": "mailto:.*",
      "program": "chrome.exe",
      "arguments": "/c ipm.note /m $1"
    },
    {
      "regex": "https://.*example.com.*",
      "program": "C:\\Program Files\\BraveSoftware\\Brave-Browser\\Application\\brave.exe",
      "arguments": "--incognito {URL}"
    },
    {
      "regex": "https://domain.com.*",
      "program": "firefox.exe",
      "arguments": "steam://openurl/{URL}"
    },
    {
      "regex": "https://(www\\.)?youtube.com.*",
      "program": "mpc-hc64.exe",
      "arguments": "-WindowStyle hidden \"C:\\Users\\mtrag\\socks_proxy.ps1\" {URL}"
    },
    {
      "regex": "ssh://(.*@(company1|company2).(com|ru))/",
      "program": "C:\\Windows\\System32\\OpenSSH\\ssh.exe",
      "arguments": "-i .ssh/id_rsa1 user1@$1"
    },
    {
      "regex": "mailto:(.*)",
      "program": "C:\\Program Files\\BraveSoftware\\Brave-Browser\\Application\\brave.exe",
      "arguments": "https://mail.google.com/mail/?view=cm\u0026fs=1\u0026to=$1"
    },
    {
      "regex": "https://music\\.yandex\\.(ru|com)/album/(\\d+)/track/(\\d+).*",
      "program": "%LOCALAPPDATA%\\Programs\\YandexMusic\\Яндекс Музыка.exe",
      "arguments": "yandexmusic://album/$2/track/$3"
    },
    {
      "regex": "https://music\\.yandex\\.(ru|com)/album/(\\d+).*",
      "program": "%LOCALAPPDATA%\\Programs\\YandexMusic\\Яндекс Музыка.exe",
      "arguments": "yandexmusic://album/$2"
    },
    {
      "regex": "ssh://(.*)/",
      "program": "C:\\Windows\\System32\\OpenSSH\\ssh.exe",
      "arguments": "-i .ssh/id_rsa_personal biba@$1"
    },
    {
      "regex": "ssh://(.*@(company1|company2).(com|ru))/",
      "program": "C:\\Windows\\System32\\OpenSSH\\ssh.exe",
      "arguments": "-i .ssh/id_rsa1 user1@$1"
    },
    {
      "regex": "interactive rule #1",
      "program": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "arguments": "{URL}"
    },
    {
      "regex": "interactive rule #2",
      "program": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "arguments": "--incognito \"{URL}\""
    },
    {
      "regex": "interactive rule #3",
      "program": "C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe",
